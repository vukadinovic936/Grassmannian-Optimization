citHeader("To cite GrassmannOptim in publications use:")

citEntry(entry = "Article",
  title        = "{GrassmannOptim}: An {R} Package for Grassmann Manifold Optimization",
  author       = personList(as.person("Kofi P. Adragni"),
                   as.person("R. Dennis Cook"),
                   as.person("Seongho Wu")),
  journal      = "Journal of Statistical Software",
  year         = "2012",
  volume       = "50",
  number       = "5",
  pages        = "1--18",
  url          = "http://www.jstatsoft.org/v50/i05/",

  textVersion  =
  paste("Kofi P. Adragni, R. Dennis Cook, Seongho Wu (2012).",
        "GrassmannOptim: An R Package for Grassmann Manifold Optimization.",
        "Journal of Statistical Software, 50(5), 1-18.",
        "URL http://www.jstatsoft.org/v50/i05/.")
)

